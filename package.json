{
  "name": "develitesse-nuxt",
  "version": "1.1.0",
  "private": true,
  "packageManager": "pnpm@9.9.0",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && pnpm simple-git-hooks install",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "nuxt typecheck --no-emit",
    "taze:up": "taze major -I",
    "test": "vitest",
    "cleanup": "nuxt cleanup && pnpm dedupe",
    "release:bump": "changelogen --bump",
    "release": "npx changelogen --release --push"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@iconify-json/heroicons": "^1.1.24",
    "@iconify-json/svg-spinners": "^1.1.3",
    "@nuxt/devtools": "^1.4.1",
    "@nuxt/eslint": "0.5.3",
    "@nuxt/image": "^1.7.0",
    "@nuxt/test-utils": "^3.14.1",
    "@nuxtjs/color-mode": "^3.4.4",
    "@pinia/nuxt": "^0.5.4",
    "@unocss/nuxt": "^0.62.3",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/nuxt": "^11.0.3",
    "eslint": "^9.9.1",
    "happy-dom": "^15.0.0",
    "lint-staged": "^15.2.9",
    "magic-regexp": "^0.8.0",
    "nuxt": "^3.13.0",
    "nuxt-radash": "^1.0.0",
    "pinia": "^2.2.2",
    "simple-git-hooks": "^2.11.1",
    "taze": "^0.16.7",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5",
    "vue-tsc": "^2.0.29"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit $1",
    "pre-commit": "npx lint-staged"
  },
  "overrides": {
    "vue": "latest"
  }
}
